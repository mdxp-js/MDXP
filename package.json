  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xo": "^0.51.0"
  },
  "xo": {
    "extends": "xo-react",
    "space": true
  }
}
